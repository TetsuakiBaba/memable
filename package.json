{
    "name": "memable",
    "version": "1.3.6",
    "description": "A temporary copy & paste memo management app.",
    "repository": "https://github.com/TetsuakiBaba/memable",
    "main": "main.js",
    "scripts": {
        "sync-version": "node sync-version.js",
        "prestart": "npm run sync-version",
        "prepackage": "npm run sync-version",
        "premake": "npm run sync-version",
        "prepublish": "npm run sync-version",
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish"
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.4.2",
        "@electron-forge/maker-deb": "^6.4.2",
        "@electron-forge/maker-dmg": "^6.0.0",
        "@electron-forge/maker-rpm": "^6.4.2",
        "@electron-forge/maker-squirrel": "^6.4.2",
        "@electron-forge/maker-zip": "^6.4.2",
        "@electron-forge/plugin-auto-unpack-natives": "^6.4.2",
        "@electron-forge/publisher-github": "^7.11.1",
        "electron": "^36.1.0"
    },
    "forge": {
        "packagerConfig": {
            "icon": "icons/icon"
        },
        "makers": [
            {
                "name": "@electron-forge/maker-zip",
                "config": {}
            },
            {
                "name": "@electron-forge/maker-dmg",
                "config": {}
            }
        ]
    },
    "dependencies": {
        "electron-squirrel-startup": "^1.0.1",
        "update-electron-app": "^3.1.2"
    }
}